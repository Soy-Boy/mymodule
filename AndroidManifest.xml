<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.module"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.mymodule.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.module.drawerlayout.DrawerLayout"
            android:label="drawerLayout"></activity>
        <activity android:name="com.module.drawerlayout.DrawerLayoutSearch"
            android:theme="@style/notitlebar"
            ></activity>
        <activity android:name="com.module.tabs.Tabs2"></activity>
        <activity android:name="com.module.arcmenu.ArcMenuActivity"></activity>
        <activity android:name="com.module.refresh.Refresh"></activity>
    </application>

</manifest>
